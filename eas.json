{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_URL": "https://api.aladhan.com/v1"
      }
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "API_URL": "https://api.aladhan.com/v1"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "API_URL": "https://api.aladhan.com/v1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
